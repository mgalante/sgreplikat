<script>define(['knockout', 'jquery'], function (ko, $) {
    return function <%=module%>(obj)
    {
        var self = this;

        <% for(var i in fields){%>
        self.<%=fields[i].name%> = ko.observable();
        <% }%>
        self.setFromJS = function(obj)
        {
            <% for(var i in fields){%>
              self.<%=fields[i].name%>(obj.<%=fields[i].name%>);
            <% }%>
        }

        if(obj){
            self.setFromJS(obj);
        }
    };
});</script>