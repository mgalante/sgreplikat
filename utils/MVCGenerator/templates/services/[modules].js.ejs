define(['helpers/ajax','jquery','knockout', 'helpers/appSettings'],function(ajax,$,ko,appSettings){
    var <%=modules%> = {
        getAll: function()
        {
            return ajax.get(appSettings.root + "<%=modules%>/list");
        },
        getById: function(id)
        {
            return ajax.get(appSettings.root + "<%=modules %>/get/" + id);
        },
        save: function(item)
        {
            return ajax.post(appSettings.root + "<%=modules%>/save", item);            
        },
        delete: function(id)
        {
            return ajax.post(appSettings.root + "<%=modules%>/delete/" + id);
        }
    };
    return <%=modules%>;
});