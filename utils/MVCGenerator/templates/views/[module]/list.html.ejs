<h1>proveedorList</h1>
<a class="btn" href='#/<%=module%>/create'>Nuevo</a>
<table class="table">
    <thead>
        <tr>
		<% for(var i in fieldsInList){%>
            <th><%=fieldsInList[i].displayName %></th>
		<% } %>
			<th></th>
        </tr>
    </thead>
    <tbody  data-bind="foreach:items">
        <tr>
		<% for(var i in fieldsInList) { %>		
            <td data-bind="text:<%=fieldsInList[i].name%>"></td>
		<% } %>
            <td><a data-bind="attr:{href: '#/<%=module%>/edit/' + id()}">Editar</a>
            &nbsp;<a href="#" data-bind="click: $parent.delete">Eliminar</a></td>
        </tr>
    </tbody>
</table>