<h1><%=Modules%></h1>

<table class="table">
    <thead>
        <tr>
		 <% for(var i in fields){%>
            <th><%=fields[i].name%></th>
		<% }%>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody  data-bind="foreach:items">
        <tr>
			<% for(var i in fields){%>
				<td data-bind="text:<%=fields[i].name%>"></td>
			<% }%>
            <td><a data-bind="attr:{href: '#/<%=module%>/edit/' + id()}">Editar</a></td>
        </tr>
    </tbody>
</table>