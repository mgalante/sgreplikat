<?php

class <%= _.str.capitalize(module)%> extends DataMapper {
    var $table = '<%=modules%>';
<%if(hasMany.length > 0){%>
        var $has_many = array(
    <%for(var i in hasMany){%>
        <%=((i>0)?',':'' )%>"<%=hasMany[i]%>"
    <%}%>
        );
<%}%>

<% if(foreignKeys.length > 0){%>
        var $has_one = array(
    <%for(var i in foreignKeys){%>
        <%=((i>0)?',':'' )%>"<%=foreignKeys[i].belongsTo.module%>"
    <%}%>
        );
<%}%>
}
